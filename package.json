    "elm-ts-interop": "^0.0.8",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "snowpack": "^3.8.8",
    "snowpack-plugin-elm": "^0.5.1"
  },
  "dependencies": {
    "@snowpack/plugin-typescript": "^1.2.1",
    "elm": "^0.19.1-5",
    "typescript": "^4.5.4"
